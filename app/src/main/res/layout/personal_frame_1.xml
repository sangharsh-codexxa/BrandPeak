<?xml version="1.0" encoding="utf-8"?>
<layout>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:sweet="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameNew"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:tag="layout/personal_frame_1_0">

    <FrameLayout
        android:id="@+id/fl_logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/linearLogo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_3ssp">

            <ImageView
                android:id="@+id/iv_logo"
                android:layout_width="@dimen/_40ssp"
                android:layout_height="@dimen/_40ssp"
                android:layout_marginStart="@dimen/_3ssp"
                android:layout_marginTop="@dimen/_10ssp"
                android:scaleType="fitCenter"
                android:src="@mipmap/ic_launcher" />
        </LinearLayout>

        <ImageView
            android:id="@+id/iv_logo_close"
            android:layout_width="@dimen/_10ssp"
            android:layout_height="@dimen/_10ssp"
            android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
            android:elevation="1dp"
            android:src="@drawable/ic_remove"
            android:visibility="gone" />
    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_6ssp"
        android:background="#ffc107" />

    <FrameLayout
        android:id="@+id/fl_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top|center_vertical|center_horizontal|center"
        android:layout_marginTop="@dimen/_4ssp"
        android:background="@drawable/red_button_background"
        android:backgroundTint="#2a9a01"
        android:paddingHorizontal="@dimen/_5ssp"
        android:paddingVertical="@dimen/_1ssp"
        android:paddingLeft="@dimen/_5ssp"
        android:paddingTop="@dimen/_1ssp"
        android:paddingRight="@dimen/_5ssp"
        android:paddingBottom="@dimen/_1ssp">

        <LinearLayout
            android:id="@+id/linearName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_name_personal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_3ssp"
                android:layout_marginLeft="@dimen/_3ssp"
                android:layout_marginRight="@dimen/_3ssp"
                android:ellipsize="end"
                android:gravity="center"
                android:maxLines="1"
                android:text="iam"
                android:textColor="@color/al_white"
                android:textSize="@dimen/_8ssp"
                android:textStyle="bold" />
        </LinearLayout>

        <ImageView
            android:id="@+id/iv_name_close"
            android:layout_width="@dimen/_10ssp"
            android:layout_height="@dimen/_10ssp"
            android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
            android:elevation="1dp"
            android:src="@drawable/ic_remove"
            android:visibility="gone" />
    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|center_vertical|center_horizontal|center"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|left|center_vertical|center_horizontal|center|start"
            android:background="@drawable/btn_rounded"
            android:gravity="left|center_vertical|center_horizontal|center|start"
            android:orientation="horizontal"
            android:paddingStart="@dimen/_10ssp"
            android:paddingTop="@dimen/_2ssp"
            android:paddingEnd="@dimen/_10ssp"
            android:paddingBottom="@dimen/_2ssp">

            <FrameLayout
                android:id="@+id/fl_website"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1ssp"
                android:layout_marginEnd="@dimen/_3ssp">

                <LinearLayout
                    android:id="@+id/linearWebsite"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/iv_website"
                        android:layout_width="@dimen/_10ssp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:src="@drawable/instagram" />

                    <TextView
                        android:id="@+id/tv_instausername_pers"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2ssp"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:maxLines="1"
                        android:text="sakhir2033"
                        android:textColor="@color/al_white"
                        android:textSize="@dimen/_7ssp" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/iv_insta_close"
                    android:layout_width="@dimen/_5ssp"
                    android:layout_height="@dimen/_5ssp"
                    android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
                    android:elevation="1dp"
                    android:src="@drawable/ic_remove"
                    android:visibility="gone" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/fl_fb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:id="@+id/linearPhone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center">

                    <ImageView
                        android:id="@+id/iv_phone_personal"
                        android:layout_width="@dimen/_10ssp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/_3ssp"
                        android:src="@drawable/facebook" />

                    <TextView
                        android:id="@+id/tv_facebookusername_pers"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2ssp"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:maxLines="1"
                        android:text="sangharsh"
                        android:textColor="@color/al_white"
                        android:textSize="@dimen/_7ssp" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/iv_fb_close"
                    android:layout_width="@dimen/_5ssp"
                    android:layout_height="@dimen/_5ssp"
                    android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
                    android:elevation="1dp"
                    android:src="@drawable/ic_remove"
                    android:visibility="gone" />
            </FrameLayout>
        </LinearLayout>



         <LinearLayout
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:orientation="horizontal">
        <FrameLayout
            android:id="@+id/fl_address"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="20dp"
            android:background="#ffc107"
            android:paddingTop="@dimen/_2ssp"
            android:paddingBottom="@dimen/_2ssp">

            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/linearAddress"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center">

                <ImageView
                    android:id="@+id/iv_address"
                    android:layout_width="@dimen/_10ssp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_2ssp"
                    android:src="@drawable/ic_email"
                    sweet:tint="@color/al_white" />

                <TextView
                    android:id="@+id/tv_address_personal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_2ssp"
                    android:ellipsize="end"
                    android:gravity="left|center_vertical|center_horizontal|center"
                    android:maxLines="1"
                    android:text="84 Vivekananda Park, Sardar Nagar"
                    android:textColor="@color/al_white"
                    android:textSize="@dimen/_7ssp" />




                <ImageView
                    android:id="@+id/iv_address_close"
                    android:layout_width="@dimen/_5ssp"
                    android:layout_height="@dimen/_5ssp"
                    android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
                    android:elevation="1dp"
                    android:src="@drawable/ic_remove"
                    android:visibility="gone" />
            </LinearLayout>



        </FrameLayout>

        <FrameLayout
            android:layout_marginEnd="@dimen/_5ssp"
            android:id="@+id/fl_phone"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:background="#ffc107"
            android:paddingTop="@dimen/_2ssp"
            android:paddingBottom="@dimen/_2ssp">

            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/linearAddress1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center">


                <TextView
                    android:drawableStart="@drawable/ic_call"
                    android:id="@+id/tv_phone_personal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_2ssp"
                    android:ellipsize="end"
                    android:gravity="left|center_vertical|center_horizontal|center"
                    android:maxLines="1"
                    android:text="676767"
                    android:textColor="@color/al_white"
                    android:textSize="@dimen/_7ssp" />

                <ImageView
                    android:id="@+id/iv_phone_close"
                    android:layout_width="@dimen/_5ssp"
                    android:layout_height="@dimen/_5ssp"
                    android:layout_gravity="top|left|center_vertical|center_horizontal|center|start"
                    android:elevation="1dp"
                    android:src="@drawable/ic_remove"
                    android:visibility="gone" />
            </LinearLayout>



        </FrameLayout>

         </LinearLayout>

    </LinearLayout>
</FrameLayout>
</layout>
